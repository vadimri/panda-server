{
  "name": "big-panda",
  "version": "0.0.1",
  "description": "The test demo",
  "main": "server.js",
  "scripts": {
    "prestart": "npm install",
    "build:test": "mocha src/**/*.spec.js --compilers js:babel-register",
    "build:js": "babel src -d build --copy-files --presets env --ignore /**/*.spec.js",
    "build": "npm run build:test &&npm run build:js",
    "test": "mocha src/**/*.spec.js --compilers js:babel-register",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "postinstall": "npm run build",
    "start": "node ./build/server.js"
  },
  "author": "Vadim",
  "license": "UNLICENSED",
  "dependencies": {
    "assert": "^1.4.1",
    "async": "^2.2.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "gravatar": "^1.6.0",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.2",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-jest": "^19.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "mocha": "^3.2.0"
  }
}
